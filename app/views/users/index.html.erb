<h1>User#index</h1>
<%= link_to 'Add', new_user_path %>
<%= link_to 'Post', new_post_path %>
<table border="1">
    <tr>
        <th>Id</th>
        <th>Email</th>
        <th>Username</th>
        <th>Password</th>
        <th>Role</th>
    </tr>
<% @users.each do |user| %>
    <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.username%></td>
        <td><%= user.passwordigest%></td>
        <td><%= user.role%></td>
        <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
<% end %>
</table>