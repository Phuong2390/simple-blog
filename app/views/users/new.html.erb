<h1>Add User</h1>
<%= form_for :user, url: users_path do |f| %>
    <p>
        <%= f.label :email %><br/>
        <%= f.text_field :email %><br/>
    </p>
    <p>
        <%= f.label :username %><br/>
        <%= f.text_field :username %><br/>
    </p>
    <p>
        <%= f.label :password %><br/>
        <%= f.text_field :passwordigest %><br/>
    </p>
    <p>
        <%= f.label :role %><br/>
        <%= f.check_box :role %><br/>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>